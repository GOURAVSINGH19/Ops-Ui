{
    "compilerOptions": {
        "outDir": "dist",
        "module": "commonjs",
        "target": "ES2015",
        "moduleResolution": "node",
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "jsx": "react-jsx"
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}